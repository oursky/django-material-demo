{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "rootDir": "src/",
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": false
  }
}
